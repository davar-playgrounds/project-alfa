pipeline {
  agent any
  environment {
    DOCKER_HUB_USER = "kbulix" // This is Docker Hub user
    PROJECT = "web-express"
  }
  stages {
    stage('myStage'){
      steps {
        sh 'pwd'
      }
    }
    stage('Build') {
      steps {
        def customImage = docker.build("${DOCKER_HUB_USER}/${PROJECT}:beta-${env.BRANCH_NAME}-${env.BUILD_NUMBER}")
        customImage.push()
      }
    }
  }
}
